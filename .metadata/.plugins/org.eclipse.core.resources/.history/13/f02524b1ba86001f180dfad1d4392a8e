package chapter04;

public class person {
	
	int id = 20230821;
	String name = "홍길순";
	
	public person() {}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	
}
